<%- include ('../partials/header')%>
<h1 class="mt-5 d-flex justify-content-around">Edit Headphone</h1>
<div class="container">
  <div class="row">
    <div style="width: 50%; margin: 3% auto">
      <form
        class="jsutify-content-around"
        method="post"
        action="/gadgets/headphones/<%=headp._id%>?_method=Put"
      >
        <div class="form-group">
          <input class="form-control" text" name="headp[ModelName]"
          value="<%=headp.ModelName%>" placeholder="Model Name" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[Brief]"
          value="<%=headp.Brief%>" placeholder="Brief" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[Description]"
          value="<%=headp.Description%>" placeholder="Description" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[Image]"
          value="<%=headp.Image%>" placeholder="Image" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[HeadphoneType]"
          value="<%=headp.HeadphoneType%>" placeholder="Headphone Type"/>
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[Price]"
          value="<%=headp.Price%>" placeholder="Price" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[Mic]"
          value="<%=headp.Mic%>" placeholder="Mic(true/false)" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[ConnectorType]"
          value="<%=headp.ConnectorType%>" placeholder="Connector type"/>
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[Connectivity]"
          value="<%=headp.Connectivity%>" placeholder="Connectivity" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[BatteryCapacity]"
          value="<%=headp.BatteryCapacity%>" placeholder="Battery Capacity" />
        </div>
        <div class="form-group">
          <input class="form-control" text" name="headp[ItemType]"
          placeholder="Item Type" value="<%=headp.ItemType%>"/>
        </div>
        <button class="btn btn-block btn-lg btn-info">Submit</button>
        <div class="form-group"></div>
      </form>
    </div>
  </div>
</div>

<%- include ('../partials/footer')%>
